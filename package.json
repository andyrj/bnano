{
  "name": "bnano",
  "version": "0.0.2",
  "description": "minimalist vdom for a minimalist h()",
  "main": "dist/bnano.js",
  "jsnext:main": "src/index.js",
  "module": "src/index.js",
  "directories": {
    "src": "src",
    "test": "test"
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "test": "nyc ava",
    "test:html": "nyc ava && nyc report --reporter=html",
    "build": "rollup --config rollup.config.prod.js",
    "build:dev": "rollup --config rollup.config.js",
    "prepublish": "npm run build && npm run test",
    "lint": "eslint --ext .js src/",
    "lint:fix": "eslint --fix --ext .js src/",
    "format": "prettier --write 'src/**/*.js'",
    "release": "npm run build && git commit -am $npm_package_version && git tag $npm_package_version && git push && git push --tags && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andyrj/bnano.git"
  },
  "author": "Andy Johnson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andyrj/bnano/issues"
  },
  "homepage": "https://github.com/andyrj/bnano#readme",
  "dependencies": {},
  "devDependencies": {
    "ava": "^0.22.0",
    "babel": "^6.23.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.26.0",
    "eslint": "^4.6.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-lodash-fp": "^2.1.3",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.3.0",
    "eslint-plugin-smells": "^1.0.1",
    "eslint-plugin-standard": "^3.0.1",
    "nyc": "^11.2.1",
    "prettier": "^1.6.1",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-babel-minify": "^3.1.2",
    "undom": "^0.3.3"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
